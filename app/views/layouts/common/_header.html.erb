<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" >
    <div class="modal-dialog">
        <div class="modal-content" style="padding-bottom:20px;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h3 class="modal-title" id="myModalLabel">Login or Sign-Up</h3>
            </div>

            <div class="modal-body">


                <div class="login_margin1">
                    <p><b>Sign-in Now! for Emangu</b>,</p>
                </div>

                <div class="login_margin_socialmedia">
                    <p><button class="btn-facebook"><i class="fa fa-facebook"></i> | Facebook</button> <button class="btn-twitter"><i class="fa fa-twitter"></i> | Twitter</button>
                        <button class="btn-google-plus"><i class="fa fa-google-plus"></i> | Google +</button><br></p>
                </div>

                <div class="login_margin2">
                    it's free and always will be..</p>

                </div>


                <form role="form" id="form_login">
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="form-group col-md-8">
                            <input type="text" class="form-control" placeholder="Enter Email-id">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="form-group col-md-8">
                            <input type="password" class="form-control" placeholder="Enter Password">
                        </div>
                    </div>
                    <div class="col-md-4"></div>
                    <button type="button" class="btn btn-primary" id="login_toggle">Login</button>
                    <button type="button" class="btn btn-success"id="register_toggle">Create New Account</button>
                    <br>
                    <div class="col-md-2"></div>
                    <a href>Trouble login ?</a>
                </form>

                <form role="form" style="display:none" id="form_register">
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="form-group col-md-8">
                            <input type="text" class="form-control" placeholder="Enter Name">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="form-group col-md-8">
                            <input type="text" class="form-control" placeholder="Enter Email-id">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="form-group col-md-8">
                            <input type="text" class="form-control" placeholder="Confirm Email-id">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2"></div>
                        <div class="form-group col-md-8">
                            <input type="password" class="form-control" placeholder="Enter Password">
                        </div>
                    </div>
                    <div class="col-md-4"></div>
                    <button type="button" class="btn btn-primary" id="login_toggle1">Login</button>
                    <button type="button" class="btn btn-success" id="register_toggle">Create New Account</button>
                </form>

            </div>


        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function() {
        $('#login_toggle1').click(function(){
            $('#form_login').show();
            $('#form_register').hide();
        })
        $('#register_toggle').click(function(){
            $('#form_login').hide();
            $('#form_register').show();
        })
    });

</script>





